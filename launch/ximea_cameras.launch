<launch>
<!-- Camera Node -->
<node name="ximea" pkg="ximea_camera" type="ximea_camera_node" output="screen" >
  <param name="frame_rate" type="int" value="60" />

  <rosparam param="camera_param_file_paths" subst_value="true">[$(find ximea_camera)/config/ximeaLeft.yaml, $(find ximea_camera)/config/ximeaRight.yaml]</rosparam>

</node>

</launch>